<!--logs.wxml-->
<view class="container log-list">
  <block wx:for="{{logs}}" wx:for-item="log" wx:key="index">
    <text class="log-item">{{index + 1}}. {{log}}</text>
  </block>
  <image src='/images/order-img.jpg' class='onlyimg'></image>
  <image src='/images/log_02_02.jpg' class='twoimg'></image>
</view>
